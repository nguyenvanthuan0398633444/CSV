@model ProjectTeamNET.Models.Response.InitDataModel
@{
    ViewData["PageName"] = " 工数管理システム";
    ViewBag.Title = "工数入力";
    string dayGet = "Day" + DateTime.Parse(Model.DateSelect).Day;
}

<div class="row">

    <main role="main" class="container-fluid px-4">
        <div class="alert alert-warning mb-2" role="alert">
            <strong>アラート</strong> - 合計工数が8h未満です！
        </div>

        @await Html.PartialAsync("_HeaderInput", Model)
        <!-- Table-->
        <input type="hidden" id="pageHistory" value="@Model.pageHistory" />
        <div class="table-responsive">
            <table class="table table-striped table-sm">
                <thead class="thead-light" id="thead">
                </thead>
                <tbody id="tbody">
                </tbody>
                <tfoot id="tfoot">
                </tfoot>
            </table>
        </div>
    </main>
</div>
@await Html.PartialAsync("_ManhourInputModal.cshtml", Model)
@section Scripts
{
    <script src="@Url.Content("~/js/manhourinput.js")"></script>
}



